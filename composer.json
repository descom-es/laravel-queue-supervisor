{
    "name": "descom/laravel-supervisor",
    "description": "Laravel package for supervisor queue:work",
    "license": "MIT",
    "authors": [
        {
            "name": "Descom Support",
            "email": "soporte@descom.es",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4",
        "orchestra/testbench": "7.5",
        "phpstan/phpstan": "^1.2",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Descom\\Supervisor\\": "src",
            "Descom\\Supervisor\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Descom\\Supervisor\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Descom\\Supervisor\\SupervisorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
